<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/colorWhite"
    android:orientation="vertical"
    tools:context="com.ylg.workspace.workspace.activity.ballgraph.ConferenceListActivity">
    <include layout="@layout/include_title"></include>

    <include layout="@layout/item_line"></include>
    <ListView
        android:id="@+id/conferencelist_lv"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:divider="@color/background_gray"
        tools:layout_editor_absoluteY="8dp"
        tools:layout_editor_absoluteX="8dp"></ListView>
</LinearLayout>
